
n = int(input())

student_marks = {}

for _ in range(n):
    line = input().split()
    name = line[0]
    scores = list(map(float, line[1:]))
    student_marks[name] = scores


query_name = input()


marks = student_marks.get(query_name)
average = sum(marks) / len(marks)


print(f"{average:.2f}")
